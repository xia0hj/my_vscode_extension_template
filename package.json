  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vscode/vsce": "^3.1.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "private": true,
  "license": "MIT",
